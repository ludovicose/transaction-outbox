{
    "name": "ludovicose/transaction-outbox",
    "description": "Laravel Pub-Sub package use transactional outbox pattern",
    "keywords": [
        "laravel",
        "pub-sub",
        "transactional-outbox"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ludovicose/transaction-outbox",
    "autoload": {
        "psr-4": {
            "Ludovicose\\TransactionOutbox\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aibek Muratbekov",
            "email": "aibek.muratbekov@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0",
        "php-amqplib/php-amqplib": "^3.1"
    },
    "require-dev": {
        "vimeo/psalm": "^4.18.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ludovicose\\TransactionOutbox\\PackageServiceProvider"
            ]
        }
    }
}
